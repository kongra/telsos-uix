{
  "name": "telsos-uix",
  "scripts": {
    "dev": "shadow-cljs -A:dev watch dev",
    "release": "shadow-cljs -A:dev release release && npm run compress",
    "compress": "gzip -k -f public/js/main.js && brotli -k -f public/js/main.js",
    "build": "shadow-cljs -A:dev compile release",
    "shadow-test": "shadow-cljs compile test",
    "shadow-watch-test": "shadow-cljs watch test"
  },
  "devDependencies": {
    "onchange": "^7.1.0",
    "process": "^0.11.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-refresh": "^0.18.0",
    "shadow-cljs": "^3.2.1",
    "tailwindcss": "^4.1.16",
    "@tailwindcss/cli": "^4.1.16"
  }
}
